// Environment variables for API keys
// Add these to your .env.local file

// Alpha Vantage - Free tier: 25 requests/day
NEXT_PUBLIC_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

// IEX Cloud - Free tier: 50,000 requests/month
NEXT_PUBLIC_IEX_CLOUD_API_KEY=your_iex_cloud_api_key_here

// Finnhub - Free tier: 60 calls/minute
NEXT_PUBLIC_FINNHUB_API_KEY=your_finnhub_api_key_here

// NewsAPI - Free tier: 1,000 requests/day
NEXT_PUBLIC_NEWS_API_KEY=your_newsapi_key_here

// Fixer.io - Free tier: 1,000 requests/month
NEXT_PUBLIC_FIXER_API_KEY=your_fixer_api_key_here

// Google Analytics 4
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

// Optional: Additional API keys
NEXT_PUBLIC_FRED_API_KEY=your_fred_api_key_here
NEXT_PUBLIC_QUANDL_API_KEY=your_quandl_api_key_here